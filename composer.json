{
    "name": "laravel/laravel",
    "type": "project",
    "description": "The Laravel Framework.",
    "keywords": ["framework", "laravel"],
    "license": "MIT",
    "require": {
        "php": "^8.1",
        "alnutile/fixtures": "0.0.4",
        "alnutile/laravel-chatgpt": "^0.0.5",
        "ankane/pgvector": "^0.1.2",
        "guzzlehttp/guzzle": "^7.2",
        "inertiajs/inertia-laravel": "^0.6.8",
        "laravel/framework": "^10.0",
        "laravel/horizon": "^5.15",
        "laravel/jetstream": "^3.1",
        "laravel/sanctum": "^3.2",
        "laravel/tinker": "^2.8",
        "opcodesio/log-viewer": "^2.4",
        "openai-php/laravel": "^0.4.1",
        "pusher/pusher-php-server": "^7.2",
        "roach-php/core": "^2.0",
        "roach-php/laravel": "^2.0",
        "smalot/pdfparser": "^2.5",
        "soundasleep/html2text": "^2.1",
        "spatie/laravel-data": "^3.4",
        "spatie/laravel-sluggable": "^3.4",
        "sundance/larachain-prompt-templates": "^0.0.1",
        "tightenco/ziggy": "^1.0",
        "voku/stop-words": "^2.0",
        "wamania/php-stemmer": "^3.0",
        "ylsideas/feature-flags": "^2.0"
    },
    "require-dev": {
        "alnutile/inertia-crud": "^1.0",
        "fakerphp/faker": "^1.9.1",
        "laravel/pint": "^1.0",
        "laravel/sail": "^1.21",
        "mockery/mockery": "^1.4.4",
        "nunomaduro/collision": "^7.0",
        "nunomaduro/larastan": "^2.5",
        "pestphp/pest": "^2.6",
        "pestphp/pest-plugin-laravel": "^2.0",
        "phpunit/phpunit": "^10.0",
        "spatie/laravel-horizon-watcher": "^1.0",
        "spatie/laravel-ignition": "^2.0"
    },
    "autoload": {
        "psr-4": {
            "App\\": "app/",
            "Database\\Factories\\": "database/factories/",
            "Database\\Seeders\\": "database/seeders/"
        },
        "files": [
            "vendor/alnutile/fixtures/src/helpers.php",
            "app/helpers.php"
        ]
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "scripts": {
        "post-autoload-dump": [
            "Illuminate\\Foundation\\ComposerScripts::postAutoloadDump",
            "@php artisan package:discover --ansi"
        ],
        "post-update-cmd": [
            "@php artisan vendor:publish --tag=laravel-assets --ansi --force"
        ],
        "post-root-package-install": [
            "@php -r \"file_exists('.env') || copy('.env.example', '.env');\""
        ],
        "post-create-project-cmd": [
            "@php artisan key:generate --ansi"
        ],
        "fix": "vendor/bin/pint",
        "stan": "vendor/bin/phpstan analyse --memory-limit 2G"
    },
    "extra": {
        "laravel": {
            "dont-discover": []
        }
    },
    "config": {
        "optimize-autoloader": true,
        "preferred-install": "dist",
        "sort-packages": true,
        "allow-plugins": {
            "pestphp/pest-plugin": true,
            "php-http/discovery": true
        }
    },
    "minimum-stability": "stable",
    "prefer-stable": true
}
